{
	"name": "dashboard",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --host",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"sass": "sass  --no-source-map --watch src/styles/index.scss static/styles/index.css",
		"copy": "copy-files-from-to",
		"start": "NODE_OPTIONS='-r dotenv/config' node index.mjs",
		"db:models": "tsx db/scripts/auto.js",
		"db:migrate": "node_modules/.bin/sequelize db:migrate "
	},
	"type": "module",
	"dependencies": {
		"@hono/swagger-ui": "^0.2.1",
		"@hono/zod-openapi": "^0.9.5",
		"@mankins/svelte-adapter-express": "^0.1.2",
		"@preact/signals-core": "^1.6.0",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-node": "^5.0.1",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@sveltestrap/sveltestrap": "^6.2.7",
		"@types/eslint": "^8.56.7",
		"@types/moment": "^2.13.0",
		"@types/shelljs": "^0.8.15",
		"bcryptjs": "^2.4.3",
		"compression": "^1.7.4",
		"copy-files-from-to": "^3.9.1",
		"dotenv": "^16.4.5",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"express": "^4.19.2",
		"fast-csv": "^5.0.1",
		"globals": "^15.0.0",
		"hono": "^4.4.13",
		"jsonwebtoken": "^9.0.2",
		"moment": "^2.30.1",
		"pg-hstore": "^2.3.4",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"redoc": "^2.1.3",
		"sass": "^1.77.4",
		"sequelize": "^6.37.2",
		"sequelize-auto": "^0.8.8",
		"sequelize-cli": "^6.6.2",
		"shelljs": "^0.8.5",
		"sqlite3": "^5.1.7",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tslib": "^2.4.1",
		"tsx": "^4.11.2",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0-alpha.20",
		"vite": "^5.0.3",
		"zod": "^3.23.8"
	}
}
